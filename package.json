{
  "name": "kahla",
  "version": "0.1.1",
  "license": "MIT",
  "scripts": {
    "electron": "npm run build && electron www/electron.js",
    "cordova": "npm run build && cordova run browser",
    "android": "npm run prod && cordova run android",
    "start": "ng serve --open",
    "build": "ng build --base-href .",
    "lint": "ng lint",
    "pack": "asar pack ./www ./www/app.asar",
    "prod": "ng build --base-href . --prod --aot --local zh --i18n-file ./src/messages.zh.xlf && npm run pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/router": "^4.4.4",
    "@angular/service-worker": "^1.0.0-beta.16",
    "cordova-android": "^6.3.0",
    "cordova-browser": "^5.0.0",
    "cordova-ios": "^4.5.1",
    "cordova-plugin-whitelist": "^1.3.2",
    "core-js": "^2.5.1",
    "font-awesome": "^4.7.0",
    "rxjs": "^5.4.3",
    "sweetalert": "^2.0.8",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.4.5",
    "@angular/compiler-cli": "^4.4.4",
    "@angular/language-service": "^4.4.4",
    "@types/jasmine": "~2.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.0.34",
    "asar": "^0.13.0",
    "codelyzer": "~3.2.1",
    "cordova": "^7.1.0",
    "electron": "^1.7.9",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "ts-node": "~3.3.0",
    "tslint": "~5.7.0",
    "typescript": "2.3.4"
  },
  "cordova": {
    "platforms": [
      "android",
      "browser",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-whitelist": {}
    }
  }
}
